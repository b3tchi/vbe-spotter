Sub Test()

    'Preparation
    Call TestPrep

    'TEST
    Set sht = ThisWorkbook.Worksheets.Add
    sht.Name = CHARW("U0159")
    
    Debug.Print sht.Name
    
    'Cleanup
    Call TestPrep
    
End Sub

Sub TestPrep()

    On Error Resume Next
    Application.DisplayAlerts = False
    Call ThisWorkbook.Worksheets(CHARW("U0159")).Delete
    Application.DisplayAlerts = True
    On Error GoTo 0

End Sub
